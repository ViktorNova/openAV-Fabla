@prefix pset: <http://lv2plug.in/ns/ext/presets#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix openav: <http://www.openavproductions.com/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .

<openav:fabla/preset#minimal>
    a           pset:Preset;
    rdfs:label "Minimal" ;
    lv2:appliesTo <http://www.openavproductions.com/fabla>;
    lv2:port [
        lv2:symbol "reverbsize" ;
        pset:value 0.2
    ] , [
        lv2:symbol "control" ;
        pset:value 0.0
    ];
    
    state:state [
    <http://www.openavproductions.com/fabla#sampleRestorePad1> <samples/click.wav> ;
    <http://www.openavproductions.com/fabla#sampleRestorePad6> <samples/perc_clack.wav>
    ] .

<openav:fabla/preset#drumnbass>
    a           pset:Preset;
    rdfs:label "Drum N Bass" ;
    lv2:appliesTo <http://www.openavproductions.com/fabla>;
    lv2:port [
        lv2:symbol "reverbsize" ;
        pset:value 0.5
    ] , [
        lv2:symbol "control" ;
        pset:value 0.5
    ];
    
    state:state [
    <http://www.openavproductions.com/fabla#sampleRestorePad1> <samples/click.wav> ;
    <http://www.openavproductions.com/fabla#sampleRestorePad6> <samples/perc_clack.wav>
    ] .
