@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix state: <http://lv2plug.in/ns/ext/state#> .
@prefix ui:    <http://lv2plug.in/ns/extensions/ui#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .

<http://www.openavproductions.com/fabla>
  a lv2:Plugin ;
  doap:name "Fabla" ;
  lv2:requiredFeature urid:map ,
    work:schedule ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:extensionData state:interface ,
    work:interface ;
  ui:ui <http://www.openavproductions.com/fabla/gui> ;
  
  lv2:port [
    a lv2:AudioPort ,
      lv2:OutputPort ;
    lv2:index 0 ;
    lv2:symbol "output_L" ;
    lv2:name "Output Left"
  ] , [
    a lv2:AudioPort ,
      lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "output_R" ;
    lv2:name "Output Right"
  ] , [
    a lv2:InputPort ,
      atom:AtomPort ;
    atom:bufferType atom:Sequence ;
    atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ,
      <http://lv2plug.in/ns/ext/patch#Message> ;
    lv2:index 2 ;
    lv2:symbol "control" ;
    lv2:name "Control"
  ] , [
    a lv2:OutputPort ,
      atom:AtomPort ;
    atom:bufferType atom:Sequence ;
    atom:supports <http://lv2plug.in/ns/ext/midi#MidiEvent> ,
                  <http://lv2plug.in/ns/ext/patch#Message> ;
    lv2:index 3 ;
    lv2:symbol "notify" ;
    lv2:name "Notify"
  ] , [
    a lv2:ControlPort ,
      lv2:InputPort ;
    lv2:index 4 ;
    lv2:symbol "reverbsize" ;
    lv2:name "Reverb Roomsize";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
  ] , [
    a lv2:ControlPort ,
      lv2:InputPort ;
    lv2:index 5 ;
    lv2:symbol "reverbwet" ;
    lv2:name "Reverb Wet";
    lv2:default 0.0 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
  ] , [
    a lv2:ControlPort ,
      lv2:InputPort ;
    lv2:index 6 ;
    lv2:symbol "mastervolume";
    lv2:name "Master Volume";
    lv2:default 0.7 ;
    lv2:minimum 0.0 ;
    lv2:maximum 1.0 ;
  ] .

<http://www.openavproductions.com/fabla/gui>
  a ui:GtkUI ;
  lv2:requiredFeature urid:map ;
  ui:portNotification [
    ui:plugin <http://www.openavproductions.com/fabla> ;
    lv2:symbol "notify" ;
    ui:notifyType atom:Sequence;
  ] .
